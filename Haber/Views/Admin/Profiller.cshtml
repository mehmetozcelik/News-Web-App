
@{
    ViewBag.Title = "Profiller - Yönetim Paneli";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


@using Haber.Models
@model List<string> 

<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading">
                Profil Listesi
            </header>
            <table class="table table-striped table-advance table-hover">
                <thead>
                    <tr>
                        <th><i class="" icon-edit"></i> Profil Adı</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (String rolAdi in Model)
                    {
                        <tr>
                            <td class="hidden-phone">@rolAdi</td>
                        </tr>
                    }


                </tbody>
            </table>
        </section>
    </div>
</div>

